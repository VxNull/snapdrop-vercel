{
	"version": 2,
	"builds": [
	  { "src": "public/**", "use": "@vercel/static" },
	  { "src": "server.js", "use": "@vercel/node" }
	],
	"routes": [
	  {
		"src": "/server/(.*)",
		"dest": "server.js",
		"headers": {
		  "Connection": "Upgrade",
		  "Upgrade": "websocket"
		}
	  },
	  { "src": "/(.*)", "dest": "/public/$1" }
	]
}
